{
  "schemaVersion": 1,
  "id": "xoverhaul",
  "version": "${version}",

  "name": "XOverhaul Mod",
  "description": "XOverhaul is a mod that improves a lot of aspects of vanilla Minecraft and expands upon them.",
  "authors": [
    "Xiaolin2"
  ],
  "contact": {
    "homepage": "",
    "sources": ""
  },

  "license": "GNU GENERAL PUBLIC LICENSE 3",
  "icon": "assets/xoverhaul/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "com.xiaolin.xoverhaul.XOverhaul"
    ],
    "client":[
      "com.xiaolin.xoverhaul.XOverhaulClient"
    ],
    "fabric-datagen" : [
      "com.xiaolin.xoverhaul.datagen.ModDataGeneratorEntrypoint"
    ]
  },
  "mixins": [
    "xoverhaul.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.6",
    "fabric": "*",
    "minecraft": "1.18.2",
    "java": ">=17",
    "xiaolib": "1.0.0"
  },
  "suggests": {
    "another-mod": "*"
  },

  "accessWidener": "xoverhaul.accesswidener"
